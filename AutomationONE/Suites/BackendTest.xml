<suite name="ApiSampleTest">

	<listeners>
		<listener class-name="Utilities.Listener.ExtentListener"></listener>
	</listeners>
 
 
	
 	 <test name="GET_USERS" enabled="true">
		<classes>
			<class name="V1.ApiTest">
				<methods>
					<include name="getUsers" />
				</methods>
			</class>
		</classes>
	</test> 
	<test name="SAVE_USER" enabled="true">
		<classes>
			<class name="V1.ApiTest">
				<methods>
					<include name="saveUser" />
				</methods>
			</class>
		</classes>
	</test>
	
